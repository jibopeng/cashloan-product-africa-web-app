    <resultMap id="borrowRaylogMap"  extends="BaseResultMap" type="com.ajaya.cashloan.cl.model.ManageBRepayLogModel">
    	 <result column="order_no" property="orderNo" jdbcType="VARCHAR" />
         <result column="borrow_amount" property="borrowAmount" jdbcType="DECIMAL" />
         <result column="repay_amount" property="repayAmount" jdbcType="DECIMAL" />
         <result column="repay_log_amount" property="repayLogAmount" jdbcType="DECIMAL" />
         <result column="state" property="state" jdbcType="VARCHAR" />
         <result column="repay_plan_time" property="repayPlanTime" jdbcType="TIMESTAMP" />
         <result column="repay_penalty" property="repayPenalty" jdbcType="DECIMAL" />
         <result column="real_name" property="realName" jdbcType="VARCHAR" />
         <result column="phone" property="phone" jdbcType="VARCHAR" />
         <result column="fee" property="fee" jdbcType="VARCHAR" />
         <result column="name" property="channelName" jdbcType="VARCHAR" />
    </resultMap>

    <select id="listModel" resultMap="borrowRaylogMap" parameterType="java.util.HashMap">
	    select
			b.order_no,b.amount as borrow_amount,b.fee,
			r.amount as repay_amount,r.penalty_amout as repay_penalty,
			r.state,l.id,l.user_id,l.borrow_id,l.amount as repay_log_amount,r.repay_time as	repay_plan_time,
			l.repay_time,l.penalty_amout,l.penalty_day,l.repay_way,l.repay_account,l.serial_number,l.refund_deduction,
			baseInfo.real_name,
            clc.name
		from cl_borrow b
		join cl_borrow_repay r on r.borrow_id=b.id
		join cl_borrow_repay_log l on l.repay_id=r.id
		join cl_user_base_info baseInfo ON b.user_id = baseInfo.user_id
		join cl_user cu ON baseInfo.user_id = cu.id
		LEFT JOIN cl_channel clc ON clc.id = cu.channel_id
		<trim prefix="where" prefixOverrides="and|or">
			<if test="borrowId != null and borrowId != ''">
				and l.borrow_id = #{borrowId,jdbcType=BIGINT}
			</if>
			<if test="orderNo != null and orderNo != ''">
				and b.order_no like concat("%",#{orderNo,jdbcType=VARCHAR},"%")
			</if>
			<if test="repayWay != null and repayWay != ''">
				and l.repay_way = #{repayWay,jdbcType=VARCHAR}
			</if>
			<if test="repayAccount != null and repayAccount != ''">
				and l.repay_account like
				concat("%",#{repayAccount,jdbcType=VARCHAR},"%")
			</if>
     <!-- 	根据实际还款时间期间数据导出   itw_yanzq start -->			
			<if test="startDate != null and startDate != ''">
                and DATE_FORMAT(l.repay_time,'%Y-%m-%d')  &gt;= #{startDate,jdbcType=TIMESTAMP}
            </if>
            <if test="endDate != null and endDate != ''">
                and DATE_FORMAT(l.repay_time,'%Y-%m-%d')  &lt;= #{endDate,jdbcType=TIMESTAMP}
            </if>
	 <!-- 	根据实际还款时间期间数据导出   itw_yanzq end -->
			<if test="type != null and type != ''">
				<if test="type =='10' ">
					and r.amount != l.amount
				</if>
				<if test="type =='20' ">
					and r.amount = l.amount
				</if>
			</if>
			and r.state=10
		</trim>
        ORDER BY l.repay_time DESC
    </select>
    
    
    <select id="listModelFq" resultMap="borrowRaylogMap" parameterType="java.util.HashMap">
	    select
			b.order_no,b.amount as borrow_amount,b.fee,
			rf.amount as repay_amount,0 as repay_penalty,
			rf.state,l.id,l.user_id,l.borrow_id,l.amount as repay_log_amount,IFNULL(rf.update_time,rf.create_time) as	repay_plan_time,
			l.repay_time,l.penalty_amout,l.penalty_day,l.repay_way,l.repay_account,l.serial_number,l.refund_deduction,
			baseInfo.real_name,
            clc.name
		from cl_borrow b
		join cl_borrow_repay_fq rf on rf.borrow_id=b.id
		join cl_borrow_repay_log_fq l on l.repay_fq_id=rf.id
		join cl_user_base_info baseInfo ON b.user_id = baseInfo.user_id
		join cl_user cu ON baseInfo.user_id = cu.id
		LEFT JOIN cl_channel clc ON clc.id = cu.channel_id
		<trim prefix="where" prefixOverrides="and|or">
			<if test="borrowId != null and borrowId != ''">
				and l.borrow_id = #{borrowId,jdbcType=BIGINT}
			</if>
			<if test="orderNo != null and orderNo != ''">
				and b.order_no like concat("%",#{orderNo,jdbcType=VARCHAR},"%")
			</if>
			<if test="repayWay != null and repayWay != ''">
				and l.repay_way = #{repayWay,jdbcType=VARCHAR}
			</if>
			<if test="repayAccount != null and repayAccount != ''">
				and l.repay_account like
				concat("%",#{repayAccount,jdbcType=VARCHAR},"%")
			</if>
     <!-- 	根据实际还款时间期间数据导出   itw_yanzq start -->			
			<if test="startDate != null and startDate != ''">
                and DATE_FORMAT(l.repay_time,'%Y-%m-%d')  &gt;= #{startDate,jdbcType=TIMESTAMP}
            </if>
            <if test="endDate != null and endDate != ''">
                and DATE_FORMAT(l.repay_time,'%Y-%m-%d')  &lt;= #{endDate,jdbcType=TIMESTAMP}
            </if>
	 <!-- 	根据实际还款时间期间数据导出   itw_yanzq end -->
			<if test="type != null and type != ''">
				<if test="type =='10' ">
					and rf.amount != l.amount
				</if>
				<if test="type =='20' ">
					and rf.amount = l.amount
				</if>
			</if>
			and rf.state=10
		</trim>
        ORDER BY l.repay_time DESC
    </select>
    
    <select id="findRepayNumbers" resultType="java.lang.Integer">
    	SELECT COUNT(1) FROM cl_borrow_repay_log WHERE user_id = #{userId,jdbcType=BIGINT} and borrow_id != #{borrowId,jdbcType=BIGINT}
    </select>
    
    <select id="findLastRepay" resultMap="borrowRaylogMap">
    	select * from cl_borrow_repay_log where user_id = #{userId,jdbcType=BIGINT} and create_time &lt;= #{createTime,jdbcType=VARCHAR} order by id desc limit 1
    </select>
    
